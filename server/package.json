{
  "name": "tractian-challenge",
  "version": "2.0.0",
  "description": "Full Stack Application developed for Tractian's challenge",
  "main": "dist/server.js",
  "type": "commonjs",
  "scripts": {
    "start": "node dist/server.js",
    "build": "tsc -p tsconfig.json",
    "ts:start": "ts-node src/server.ts",
    "dev": "tsnd --respawn --transpile-only --exit-child src/server.ts",
    "debug": "tsnd --respawn --transpile-only --exit-child --debug src/server.ts",
    "lint": "npx eslint --fix --ext .ts src/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "typescript",
    "node",
    "nodejs",
    "mongodb",
    "mongoose"
  ],
  "author": {
    "name": "Nivaldo Farias",
    "url": "https://github.com/NivaldoFarias"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^1.1.2",
    "bcrypt": "^5.0.1",
    "chalk": "^4.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "helmet": "^5.1.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.6.4",
    "mongoose-unique-validator": "^3.1.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mongoose-unique-validator": "^1.0.6",
    "@types/node": "^18.0.3",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "ts-node": "^10.8.2",
    "ts-node-dev": "^1.1.8",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4"
  }
}
